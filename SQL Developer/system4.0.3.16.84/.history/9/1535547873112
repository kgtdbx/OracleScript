SELECT NVL( "dt_fafnir_Q_JOINER"."UPPF_EFTIRST_FUNDARG" , 0) ,
    NVL( "dt_fafnir_Q_JOINER"."AFALLNIR_VEXTIR_FUNDARG" , 0) ,
    NVL( "dt_fafnir_Q_JOINER"."VANSKIL_FUNDARGENGI" , 0) ,
    NVL( "dt_fafnir_Q_JOINER"."AFALLNIR_PIK_VEXTIR_FUNDARG" , 0) ,
    "DIM_LOAN"."LOAN_DATE_IN_ARREARS" ,
    "dt_fafnir_Q_JOINER"."LAST_BANKING_DAY" ,
    "dt_fafnir_Q_JOINER"."INNLAN_UTLAN" ,
    "DIM_LOAN"."DIMENSION_KEY" ,
    "dt_fafnir_Q_JOINER"."GJALDDAGAG_GREIDSLUDAGSG" ,
    "DIM_CUSTOMER"."DIMENSION_KEY" ,
    "dt_fafnir_Q_JOINER"."UPPF_EFTIRST_FUNDARG" ,
    "dt_fafnir_Q_JOINER"."LOAN_BALANCE" ,
    "dt_fafnir_Q_JOINER"."VERDBAETUR_FUNDARGENGI" ,
    "dt_fafnir_Q_JOINER"."VANSKIL_FUNDARGENGI" ,
    "dt_fafnir_Q_JOINER"."GJALDF_VEXTIR_FUNDARG" ,
    "dt_fafnir_Q_JOINER"."DRATTARVEXTIR_FUNDARG" ,
    "dt_fafnir_Q_JOINER"."AFFOLL_FUNDARG" ,
    "dt_fafnir_Q_JOINER"."AFALLNIR_PIK_VEXTIR_FUNDARG" ,
    "dt_fafnir_Q_JOINER"."AFALLNIR_VEXTIR_FUNDARG" ,
    "dt_fafnir_Q_JOINER"."SKULDABREF_NUMER" ,
    "dt_fafnir_Q_JOINER"."LANSNUMER" ,
    "DIM_LOAN"."ID" ,
    "dt_fafnir_Q_JOINER"."UPPRUNI" ,
    "DIM_LOAN"."SOURCE_SYSTEM" ,
    "dt_fafnir_Q_JOINER"."VSKM_NUMBER" ,
    "DIM_CUSTOMER"."CUST_ID" ,
    "DIM_CUSTOMER"."CUST_COUNTRY_CODE" ,
    "dt_fafnir_Q_JOINER"."MYNT" ,
    "dt_fafnir_Q_JOINER"."CURRENCY_ID" ,
    "dt_fafnir_Q_JOINER"."PROFIT_CENTER" ,
    "dt_fafnir_Q_JOINER"."SAP_ACCOUNT" ,
    "dt_fafnir_Q_JOINER"."REFERENCE3" ,
    "dt_fafnir_Q_JOINER"."COMPANY_CODE" ,
    "DIM_CUSTOMER"."INDUSTRY_SEGMENT" ,
    "dt_fafnir_Q_JOINER"."LANSTEGUND"
FROM ("VALHOLL"."DI_FCT_LOAN_BALANCE_FAFNIR" "dt_fafnir_Q_JOINER"
LEFT OUTER JOIN "VALHOLL"."DIM_CUSTOMER" "DIM_CUSTOMER"
ON ( "dt_fafnir_Q_JOINER"."VSKM_NUMBER" = "DIM_CUSTOMER"."CUST_ID" ))
LEFT OUTER JOIN "VALHOLL"."DIM_LOAN" "DIM_LOAN"
ON ( "dt_fafnir_Q_JOINER"."LANSNUMER"     = "DIM_LOAN"."ID" )
AND ( "dt_fafnir_Q_JOINER"."UPPRUNI"      = "DIM_LOAN"."SOURCE_SYSTEM" )
WHERE ( "dt_fafnir_Q_JOINER"."TRUE_DATE" >= NVL( "DIM_LOAN"."VALID_FROM" , "dt_fafnir_Q_JOINER".
    "TRANSACTION_DATE" ))
AND ( "dt_fafnir_Q_JOINER"."TRUE_DATE" <= NVL( "DIM_LOAN"."VALID_TO" , (sysdate + 100)))
AND ( "dt_fafnir_Q_JOINER"."TRUE_DATE" >= NVL( "DIM_CUSTOMER"."VALID_FROM" , "dt_fafnir_Q_JOINER".
    "TRANSACTION_DATE" ))
AND ( "dt_fafnir_Q_JOINER"."TRUE_DATE" <= NVL( "DIM_CUSTOMER"."VALID_TO" , (sysdate + 100)))

