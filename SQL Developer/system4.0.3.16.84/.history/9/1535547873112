SELECT SUBSTR(rtrim(TO_CHAR( "STG_FAFNIR_SKBR_GREIDSLUFL"."SKULDABREF_NUMER" ), ','), 1, 35) ,
    ( ((
    CASE
        WHEN ( "STG_FAFNIR_SKBR_GREIDSLUFL"."INNLAN_UTLAN" = 'I')
        THEN 'l'
        ELSE 'a'
    END) ) ) ,
    "DIM_LOAN"."DIMENSION_KEY" ,
    "DIM_CUSTOMER"."DIMENSION_KEY" ,
    "DIM_LOAN"."LOAN_TYPE" ,
    "DIM_LOAN"."CURRENCY" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."DAGSETNING" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."VEXTIR" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."UPPHAED" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."GENGI" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."REFERENCE_DATE" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."VERDTRYGGING" ,
    "STG_FAFNIR_SKBR_STADA_BREF"."FUNDARGENGI" ,
    "DIM_CUSTOMER"."CUST_COUNTRY_CODE" ,
    "DIM_CUSTOMER"."CUST_ID" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."DEILD" ,
    "STG_FAFNIR_SKBR_STADA_BREF"."VEXTIR_GETA_BREYST_NAEST" ,
    "STG_FAFNIR_SKBR_STADA_BREF"."LOKAGJALDDAGI" ,
    "DIM_CUSTOMER"."INDUSTRY_SEGMENT" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."EFTIRSTODVAR" ,
    "STG2_GL"."REFERENCE3" ,
    "STG2_GL"."COMPANY_CODE" ,
    "DIM_LOAN"."ID" ,
    "STG2_GL"."SAP_ACCOUNT" ,
    "STG2_GL"."PROFIT_CENTER" ,
    "STG_FAFNIR_SKBR_GREIDSLUFL"."SKULDABREF_NUMER" ,
    "BANKING_DAY"."TRUE_DATE" ,
    "STG_FAFNIR_SKBR_STADA_BREF"."KENNITALA" ,
    "DIM_CUSTOMER"."VALID_FROM" ,
    "DIM_CUSTOMER"."VALID_TO" ,
    "DIM_LOAN"."SOURCE_SYSTEM" ,
    "DIM_LOAN"."VALID_FROM" ,
    "DIM_LOAN"."VALID_TO" ,
    "STG_FAFNIR_SKBR_STADA_BREF"."SKULDABREF_NUMER" ,
    "STG2_GL"."LOAN_ID" ,
    "STG2_GL"."SYSTEM_ID" ,
    "BANKING_DAY"."TRANSACTION_DATE"
FROM (((("BIFROST"."STG_FAFNIR_SKBR_GREIDSLUFL" "STG_FAFNIR_SKBR_GREIDSLUFL"
LEFT OUTER JOIN "BALDUR"."BANKING_DAY" "BANKING_DAY"
ON ( "STG_FAFNIR_SKBR_GREIDSLUFL"."REFERENCE_DATE" = "BANKING_DAY"."TRANSACTION_DATE" ))
LEFT OUTER JOIN "BIFROST"."STG2_GL" "STG2_GL"
ON (SUBSTR(rtrim(TO_CHAR( "STG_FAFNIR_SKBR_GREIDSLUFL"."SKULDABREF_NUMER" ), ','), 1, 35) =
    "STG2_GL"."LOAN_ID" )
AND ( "STG2_GL"."SYSTEM_ID" = 'FAFNIR'))
LEFT OUTER JOIN "BIFROST"."STG_FAFNIR_SKBR_STADA_BREF" "STG_FAFNIR_SKBR_STADA_BREF"
ON (SUBSTR(rtrim(TO_CHAR( "STG_FAFNIR_SKBR_GREIDSLUFL"."SKULDABREF_NUMER" ), ','), 1, 35) = SUBSTR(
    rtrim(TO_CHAR( "STG_FAFNIR_SKBR_STADA_BREF"."SKULDABREF_NUMER" ), ','), 1, 35)))
LEFT OUTER JOIN "VALHOLL"."DIM_LOAN" "DIM_LOAN"
ON (SUBSTR(rtrim(TO_CHAR( "STG_FAFNIR_SKBR_GREIDSLUFL"."SKULDABREF_NUMER" ), ','), 1, 35) =
    "DIM_LOAN"."ID" )
AND ( "DIM_LOAN"."SOURCE_SYSTEM" = 'FAFNIR')
AND ( "BANKING_DAY"."TRUE_DATE" >= "DIM_LOAN"."VALID_FROM" )
AND ( "BANKING_DAY"."TRUE_DATE" <= NVL( "DIM_LOAN"."VALID_TO" , (sysdate + 100))))
LEFT OUTER JOIN "VALHOLL"."DIM_CUSTOMER" "DIM_CUSTOMER"
ON ( "STG_FAFNIR_SKBR_STADA_BREF"."KENNITALA" = "DIM_CUSTOMER"."CUST_ID" )
AND ( "BANKING_DAY"."TRUE_DATE"              >= "DIM_CUSTOMER"."VALID_FROM" )
AND ( "BANKING_DAY"."TRUE_DATE"              <= NVL( "DIM_CUSTOMER"."VALID_TO" , (sysdate + 100)))
